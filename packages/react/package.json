{
  "name": "@strail/react",
  "sideEffects": false,
  "version": "0.0.1",
  "license": "MIT",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build:declarations": "tsc --jsx react-jsx --declaration --emitDeclarationOnly --outdir lib src/components/index.ts",
    "build:components": "yarn esbuild src/components/index.ts --bundle --outdir=lib --external:'react' --external:'clsx' --target=esnext --allow-overwrite --format=esm --jsx=transform",
    "build": "run-p build:*",
    "watch": "yarn esbuild src/components/index.ts --bundle --watch --outdir=lib --external:'react' --external:'clsx' --target=esnext --allow-overwrite --jsx-dev --format=esm",
    "lint": "eslint src/**/*.{ts,tsx} && prettier src --check",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix && prettier src --write"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@storybook/builder-vite": "0.2.4",
    "css-loader": "5.2.6",
    "esbuild": "0.14.51",
    "normalize.css": "8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "1.55.0",
    "sass-loader": "10.1.1",
    "style-loader": "2.0.0",
    "typescript": "4.7.4",
    "vite": "3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.12",
    "@storybook/theming": "6.5.12",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.10",
    "@types/react-test-renderer": "^17.0.1",
    "babel-loader": "^8.2.3",
    "clsx": "^1.1.1",
    "css-loader": "^6.5.1",
    "cypress": "^8.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "react-polymorphic-box": "^3.0.3",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
